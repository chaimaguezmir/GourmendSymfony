{% extends 'base.html.twig' %}

{% block title %}ProductRating index{% endblock %}

{% block body %}
    <h1>ProductRating index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nbrratting</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for product_rating in product_ratings %}
            <tr>
                <td>{{ product_rating.id }}</td>
                <td>{{ product_rating.nbrratting }}</td>
                <td>
                    <a href="{{ path('app_product_rating_show', {'id': product_rating.id}) }}">show</a>
                    <a href="{{ path('app_product_rating_edit', {'id': product_rating.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
